<!--
The 'install' script.
-->
<FILE Run="/bin/bash">
<INLINE>
timer=30
while [[ $timer -gt 0 ]]; do
  gw=$(route -n|awk '/^0.0.0.0/{print $2}')
  if [[ -n $gw &amp;&amp; $(ping -qnc1 $gw|awk '/received/{print $4}') -eq 1 ]]; then
    break
  fi
  ((timer--))
  sleep 1
done
if [[ $timer -eq 0 ]]; then
  echo "No network communication !!!"
fi
</INLINE>
</FILE>

